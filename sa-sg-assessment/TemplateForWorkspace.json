				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": [],
				"lastPublishTime": "2023-12-04T21:25:54Z"
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/sqlPools/sqlPoolSGAssessment')]"
			]
		},
